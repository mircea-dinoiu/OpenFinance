  "scripts": {
    "ci": "run-p --aggregate-output test-all lint-all",
    "build": "rm -rf build && react-scripts build && mv build/index.html build/app.html",
    "dev": "react-scripts start",
    "start": "node ./bin/www",
    "start:ts": "TS_NODE_PROJECT=tsconfig.server.json ts-node server/index.ts",
    "test": "react-scripts test",
    "test-related": "react-scripts test --findRelatedTests",
    "test-all": "react-scripts test --watchAll=false",
    "test-cov": "open build/coverage/index.html",
    "lint": "eslint --quiet --ext .js,.ts.,.tsx",
    "lint-all": "yarn lint server src",
    "format": "prettier-eslint --write",
    "format-all": "yarn run format \"server/**/*.js\" \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\" \"*.js\"",
    "nodemon": "nodemon --ignore 'sources/**/*.ts' --ignore 'sources/**/*.tsx' --ignore '*.test.js*' --ignore storage/"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn run format",
      "yarn run lint",
      "git add"
    ]
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 safari version",
    "last 1 firefox version"
  ],
  "proxy": "http://localhost:3000/",
  "homepage": "/"
}
